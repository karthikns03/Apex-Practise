public class ApexDay9 {

    	Integer x=10;
    	public static integer y=20;
    
    public void setValues(Integer a) {
        x=a;
        y=a;
        
    }
    
    public void displayValues() {
        system.debug('Non static x='+x);
        system.debug('Static y ='+y);
    }
}